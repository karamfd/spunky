      "settings": {
        "foreground": "#00d6cb"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "constant.language",
        "entity.name.method.js",
        "keyword",
        "keyword.other.special-method",
        "support.constant",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#00d6cb"
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.character",
        "punctuation.definition.entity.html"
      ],
      "settings": {
        "foreground": "#fad000"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape",
        "constant.escape",
        "constant.other.scss",
        "support.constant.media.css"
