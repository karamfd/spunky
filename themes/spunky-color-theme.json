  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8390afcc"
      }
    },
    {
      "name": "CSS Property Value, Entity and Media Queries",
      "scope": [
        "entity",
        "entity.name.type.class.js",
        "support.class"
      ],
      "settings": {
        "foreground": "#ff978f"
      }
    },
    {
      "name": "Numbers & Hex Colors",
      "scope": [
        "constant.numeric",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ff978f"
      }
    },
    {
      "name": "CSS - Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#ff978f"
      }
    },
    {
      "name": "CSS Classes and IDs",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#ff978f"
      }
    },
    {
      "name": "CSS - Pseudo Classes and Elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#c2adff"
      }
    },
    {
      "name": "JS and PHP - Class, Functions, and Conditionals",
      "scope": [
        "storage",
        "keyword.control.conditional.js",
        "keyword.operator.class.php"
      ],
      "settings": {
        "foreground": "#fad000"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "entity.other.inherited-class.js",
        "keyword.key.toml",
        "source.toml keyword",
        "string constant.other.placeholder",
        "support",
        "variable"
      ],
      "settings": {
        "foreground": "#bcbfc8"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.deprecated",
        "invalid.illegal",
      ],
      "settings": {
        "foreground": "#ffbf70"
      }
    },
    {
      "name": "Storage, Template Literals",
      "scope": [
        "punctuation.definition.keyword",
        "punctuation.definition.template-expression",
        "source.toml string",
      ],
      "settings": {
        "foreground": "#00d6cb"
      }
    },
    {
      "name": "JS and PHP - Storage Modifier",
      "scope": [
        "storage.modifier.js",
        "storage.modifier.php",
        "storage.modifier.async.js",
        "storage.modifier.extends.php"
      ],
      "settings": {
        "foreground": "#00d6cb"
      }
    },
    {
      "name": "PHP Variables",
      "scope": [
        "punctuation.definition.variable.php",
        "source.php variable.language",
        "variable.other.php",
        "variable.other.global.safer.php"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant",
        "keyword.other.substitution",
        "keyword.other.template",
        "meta.brace",
        "meta.tag",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        "punctuation.separator.inheritance.php",
        "source.css support"
      ],
      "settings": {
        "foreground": "#bcbfc8"
      }
    },
    {
      "name": "Meta and Tag",
      "scope": [
        "entity.name.tag",
        "markup.deleted.git_gutter",
        "meta.tag.sgml",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.html"
