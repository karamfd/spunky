      "settings": {
        "foreground": "#006ebd"
      }
    },
    {
      "name": "JS - Storage Modifier",
      "scope": [
        "storage.modifier.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#006ebd"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
