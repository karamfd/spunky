  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#262c3bab"
      }
    },
    {
      "name": "CSS Property Value, Entity and Media Queries",
      "scope": [
        "entity",
        "entity.name.type.class.js",
        "support.class"
      ],
      "settings": {
        "foreground": "#ad004e"
      }
    },
    {
      "name": "Numbers - JS and PHP",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#00755a"
      }
    },
    {
      "name": "Numbers & Hex Colors",
      "scope": [
        "constant.numeric.float.toml",
        "constant.numeric.integer.toml",
        "constant.numeric.float.yaml",
        "constant.numeric.integer.yaml",
        "source.css constant.numeric",
        "source.css constant.other.color",
        "source.json constant.numeric",
        "source.sql constant.numeric",
        "source.toml constant.numeric",
        "source.yaml constant.numeric"
      ],
      "settings": {
        "foreground": "#ad004e"
      }
    },
    {
      "name": "CSS - Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#262c3b"
      }
    },
    {
      "name": "CSS Classes and IDs",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#00755a"
      }
    },
    {
      "name": "CSS - Pseudo Classes and Elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#756b00"
      }
    },
    {
      "name": "JS - Functions",
      "scope": [
        "punctuation.definition.keyword",
        "storage.type.class",
        "storage.type.function",
        "string.quoted.single.basic.line.toml",
        "string.quoted.single.literal.line.toml",
        "source.toml string"
      ],
      "settings": {
        "foreground": "#006ebd"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "entity.other.inherited-class.js",
        "keyword.key.toml",
        "string constant.other.placeholder",
        "source.toml keyword",
        "support",
        "variable"
      ],
      "settings": {
        "foreground": "#262c3b"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
