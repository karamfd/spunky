  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#c4cbd9a6"
      }
    },
    {
      "name": "CSS Property Value, Entity and Media Queries",
      "scope": [
        "entity",
        "entity.name.type.class.js",
        "support",
        "support.class"
      ],
      "settings": {
        "foreground": "#e0bb00"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.other.color"],
      "settings": {
        "foreground": "#ff99a1"
      }
    },
    {
      "name": "CSS - String and Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c294ff"
      }
    },
    {
      "name": "CSS Pseudo Classes and Elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#00d5a4"
      }
    },
    {
      "name": "Numbers and Booleans - Config Files",
      "scope": [
        "constant.language.boolean.yaml",
        "constant.language.json",
        "constant.numeric.json",
        "constant.numeric.integer.toml",
        "constant.numeric.integer.yaml",
        "constant.other.boolean.toml",
        "constant.other.date.toml",
        "constant.other.timestamp.yaml",
        "source.toml constant.other.boolean"
      ],
      "settings": {
        "foreground": "#e0bb00"
      }
    },
    {
      "name": "String and Keyword",
      "scope": [
        "punctuation.definition.keyword",
        "storage.type.class",
        "string.quoted.single.basic.line.toml",
        "string.quoted.single.literal.line.toml",
        "source.toml string"
      ],
      "settings": {
        "foreground": "#6bceff"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "entity.other.inherited-class.js",
        "keyword.key.toml",
        "string constant.other.placeholder",
        "source.toml keyword",
        "variable"
      ],
      "settings": {
        "foreground": "#c4cbd9"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff6060"
      }
    },
    {
      "name": "Storage and PHP Variables",
      "scope": [
        "storage",
        "variable.other.php"
      ],
      "settings": {
        "foreground": "#6bceff"
      }
    },
    {
      "name": "Storage Modifier and Function",
      "scope": [
        "keyword.operator.class.php",
        "punctuation.definition.template-expression",
        "storage.modifier.js",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#c294ff"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant",
        "keyword.other.substitution",
        "keyword.other.template",
        "meta.brace",
        "meta.tag",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        "punctuation.separator.inheritance.php",
        "source.css support"
      ],
      "settings": {
        "foreground": "#c4cbd9"
      }
    },
    {
      "name": "Meta and Tag",
      "scope": [
        "entity.name.tag",
        "markup.deleted.git_gutter",
        "meta.tag.sgml",
        "meta.embedded.block.frontmatter",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#6bceff"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "constant.language",
        "entity.name.method.js",
        "keyword",
        "keyword.other.special-method",
        "support.function",
        "variable.function",
        "variable.other.global.safer.php"
      ],
      "settings": {
        "foreground": "#6bceff"
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": ["punctuation.definition.string", "string"],
      "settings": {
        "foreground": "#00d5a4"
      }
    },
    {
      "name": "Escape Characters and HTML String",
      "scope": [
        "constant.character",
        "constant.character.escape",
        "constant.escape",
        "constant.other.scss",
        "punctuation.definition.entity.html",
        "support.constant"
      ],
      "settings": {
        "foreground": "#ff99a1"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "constant.other.key",
        "constant.other.symbol",
        "markup.inserted",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "string.other.link",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#ff99a1"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "markup.changed",
        "markup.changed.git_gutter",
        "meta.use.php",
        "source.sql constant.numeric",
        "support.orther.namespace.use.php",
        "support.other.namespace.php",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#e0bb00"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "markup.deleted",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#c294ff"
      }
    },
    {
      "name": "Language methods",
      "scope": "variable.language",
      "settings": {
        "foreground": "#6bceff"
      }
    },
    {
      "name": "URL",
      "scope": ["*link*", "*uri*", "*url*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#e0bb00"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": "source.js constant.other.object.key.js string.unquoted.label.js",
      "settings": {
        "fontStyle": "",
        "foreground": "#c294ff"
      }
    },
