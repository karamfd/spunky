  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#b1b6c4ad"
      }
    },
    {
      "name": "CSS Property Value, Entity and Media Queries",
      "scope": [
        "entity",
        "entity.name.type.class.js",
        "support",
        "support.class"
      ],
      "settings": {
        "foreground": "#d6b200"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.other.color"],
      "settings": {
        "foreground": "#ff7787"
      }
    },
    {
      "name": "CSS - Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00c774"
      }
    },
    {
      "name": "CSS Classes and IDs",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#ba85ff"
      }
    },
    {
      "name": "Numbers - JS and PHP",
      "scope": ["source.js constant.numeric", "source.php constant.numeric"],
      "settings": {
        "foreground": "#00c774"
      }
    },
    {
      "name": "JS - Functions",
      "scope": [
        "punctuation.definition.keyword",
        "storage.type.class",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#3aadff"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "entity.other.inherited-class.js",
        "string constant.other.placeholder",
        "variable"
      ],
      "settings": {
        "foreground": "#b1b6c4"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff6060"
      }
    },
    {
      "name": "Storage, Template Literals and PHP Variables",
      "scope": [
        "punctuation.definition.template-expression",
        "storage",
        "variable.other.php"
      ],
      "settings": {
        "foreground": "#3aadff"
      }
    },
    {
      "name": "JS - Storage Modifier",
      "scope": ["storage.modifier.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3aadff"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant",
        "keyword.other.substitution",
        "keyword.other.template",
        "meta.brace",
        "meta.tag",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        "punctuation.separator.inheritance.php",
        "source.css support"
      ],
      "settings": {
        "foreground": "#b1b6c4"
      }
    },
    {
      "name": "Meta and Tag",
      "scope": [
        "entity.name.tag",
        "markup.deleted.git_gutter",
        "meta.tag.sgml",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#3aadff"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "constant.language",
        "entity.name.method.js",
        "keyword",
        "keyword.other.special-method",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#3aadff"
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": ["constant.character", "punctuation.definition.entity.html"],
      "settings": {
        "foreground": "#00c774"
      }
    },
    {
      "name": "Escape Characters and HTML String",
      "scope": [
        "constant.character.escape",
        "constant.escape",
        "constant.other.scss",
        "support.constant"
      ],
      "settings": {
        "foreground": "#ff7787"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "constant.other.key",
        "constant.other.symbol",
        "markup.inserted",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "punctuation.definition.string",
        "string",
        "string.other.link",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#ff7787"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "markup.changed",
        "markup.changed.git_gutter",
        "meta.use.php",
        "source.sql constant.numeric",
        "support.orther.namespace.use.php",
        "support.other.namespace.php",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#d6b200"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "markup.deleted",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#ba85ff"
      }
    },
    {
      "name": "Language methods",
      "scope": "variable.language",
      "settings": {
        "foreground": "#3aadff"
      }
    },
    {
      "name": "URL",
      "scope": ["*link*", "*uri*", "*url*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#d6b200"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": "source.js constant.other.object.key.js string.unquoted.label.js",
      "settings": {
        "fontStyle": "",
        "foreground": "#ba85ff"
      }
    },
    {
